<drawables>
    <bitmap id="LauncherIcon" filename="LauncherIcon.png" />
    <drawable-list id="black_background">
        <shape type="rectangle" color="Gfx.COLOR_BLACK" x="0" y="0" width="fill" height="fill" />
    </drawable-list>
    <drawable-list id="white_background">
        <shape type="rectangle" color="Gfx.COLOR_WHITE" x="0" y="0" width="fill" height="fill" />
    </drawable-list>
    <drawable-list id="horizontal_center_bar">
        <shape type="rectangle" color="Gfx.COLOR_DK_GRAY" x="0" y="center" width="fill" height="10" />
    </drawable-list>
    <drawable-list id="vertical_center_bar">
        <shape type="rectangle" color="Gfx.COLOR_DK_GRAY" x="center" y="0" width="5" height="fill" />
    </drawable-list>
    <drawable-list id="vertical_title_bar">
        <shape type="rectangle" color="Gfx.COLOR_DK_GRAY" x="0" y="75" width="fill" height="10" />
    </drawable-list>    
</drawables>
